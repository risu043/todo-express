{
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register --files ./bin/run_server.ts",
    "start:test": "dotenv -e .env.test -- npm run start",
    "watch": "nodemon -r tsconfig-paths/register --files --watch ./src ./bin/run_server.ts",
    "db:migrate": "prisma migrate dev",
    "db:migrate:test": "dotenv -e .env.test -- prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:seed:test": "dotenv -e .env.test -- prisma db seed",
    "db:reset": "prisma migrate reset",
    "db:reset:test": "dotenv -e .env.test -- prisma migrate reset --force",
    "test": "npm run db:reset:test && dotenv -e .env.test -- jest -c ./e2e/jest.config.js",
    "test:index": "npm run test -- index.ts",
    "test:show": "npm run test -- show.ts",
    "test:create": "npm run test -- create.ts",
    "test:update": "npm run test -- update.ts",
    "test:delete": "npm run test -- delete.ts",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format:check": "prettier --check '**/*.{js,ts}'",
    "format:fix": "prettier --write '**/*.{js,ts}'",
    "typecheck": "tsc --noEmit --skipLibCheck --project tsconfig.json",
    "check_puppeteer": "npm run test -- check_puppeteer.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.14.0",
    "cookie-parser": "^1.4.6",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-async-errors": "^3.1.1",
    "express-ejs-layouts": "^2.5.1",
    "express-validator": "^7.1.0",
    "helmet": "^7.1.0",
    "method-override": "^3.0.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "prisma": "^5.14.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@quramy/jest-prisma": "^1.8.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/express-ejs-layouts": "^2.5.4",
    "@types/jest": "^29.5.12",
    "@types/jest-environment-puppeteer": "^5.0.6",
    "@types/method-override": "^0.0.35",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-puppeteer": "^10.1.1",
    "prettier": "^3.3.2",
    "puppeteer": "^23.3.0",
    "ts-jest": "^29.1.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  }
}
